{
  "version": 2,
  "builds": [
    {
      "src": "./backend/serverOne.js", 
      "use": "@vercel/node"
    },
    {
      "src": "./frontend/myecommerce-app/build", 
      "use": "@vercel/static"
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/backend/serverOne.js"
    }
  ]
}